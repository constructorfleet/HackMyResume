{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "info": {
            "type": "object",
            "properties": {
                "image": {
                    "type": "string"
                },
                "label": {
                    "type": "string"
                },
                "brief": {
                    "type": "string"
                }
            }
        },
        "social": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "network": {
                        "type": "string"
                    },
                    "user": {
                        "type": "string"
                    },
                    "url": {
                        "type": "string"
                    }
                }
            }
        }
    },
    "contact": {
        "type": "object",
        "properties": {
            "email": {
                "type": "string"
            },
            "phone": {
                "type": "string"
            },
            "website": {
                "type": "string"
            }
        }
    },
    "location": {
        "type": "object",
        "properties": {
            "address": {
                "type": "string"
            },
            "city": {
                "type": "string"
            },
            "region": {
                "type": "string"
            },
            "code": {
                "type": "string"
            }
        }
    },
    "speaking": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "event": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "date": {
                    "type": "string"
                },
                "location": {
                    "type": "string"
                },
                "summary": {
                    "type": "string"
                },
                "highlights": {
                    "type": "array",
                    "items": [
                        {
                            "type": "string"
                        }
                    ]
                }
            }
        }
    },
    "employment": {
        "type": "object",
        "properties": {
            "history": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "employer": {
                            "type": "string"
                        },
                        "start": {
                            "type": "string"
                        },
                        "end": {
                            "type": "string"
                        },
                        "position": {
                            "type": "string"
                        },
                        "url": {
                            "type": "string"
                        },
                        "highlights": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "keywords": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "education": {
        "type": "object",
        "properties": {
            "level": {
                "type": "string"
            },
            "history": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "institution": {
                            "type": "string"
                        },
                        "area": {
                            "type": "string"
                        },
                        "studyType": {
                            "type": "string"
                        },
                        "start": {
                            "type": "string"
                        },
                        "end": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "skills": {
        "type": "object",
        "properties": {
            "list": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "level": {
                            "type": "string"
                        },
                        "years": {
                            "type": "integer"
                        }
                    }
                }
            }
        }
    }
}